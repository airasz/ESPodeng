<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_about"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/black"
    tools:context="chicken.head.espodeng.AturActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingLeft="10dip"
        android:paddingRight="10dip"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="8dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="12dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_setting_title"
                android:layout_width="389dp"
                android:layout_height="wrap_content"
                android:fontFamily="sans-serif-condensed"
                android:freezesText="false"
                android:gravity="top"
                android:singleLine="true"
                android:text="setting"
                android:textColor="@android:color/white"
                android:textSize="68sp" />

            <TextView
                android:id="@+id/tv_setting_stitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="espodeng"
                android:textColor="@android:color/white"
                android:textSize="18sp" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="40dip" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="8dp">

            <TextView
                android:id="@+id/tv_set_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:text="random color scheme"
                android:textColor="@android:color/white" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="52dp"
                android:gravity="right"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btn_rnd_col_bool"
                    android:layout_width="54dp"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/holo_green_dark"
                    android:elevation="0dp"
                    android:text="aktif"
                    android:textColor="@android:color/white" />

                <Button
                    android:id="@+id/btn_rnd_col_bool2"
                    android:layout_width="wrap_content"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/darker_gray"
                    android:elevation="0dp"
                    android:text="non aktif"
                    android:textColor="@android:color/white" />
            </LinearLayout>

        </LinearLayout>

        <RelativeLayout
            android:id="@+id/ll_PC"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_set_name2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:layout_toLeftOf="@+id/linearLayout3"
                android:text="choose scheme color "
                android:textColor="@android:color/white" />

            <LinearLayout
                android:id="@+id/linearLayout3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:orientation="horizontal"
                android:padding="2dip">

                <Button
                    android:id="@+id/btn_open_colorD"
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@color/colorAccent"
                    android:elevation="3dp"
                    android:text=" " />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/ll_alarm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1">

            <TextView
                android:id="@+id/tv_set_name3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:text="Auto pause saat headset tercabut"
                android:textColor="@android:color/white" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="52dp"
                android:gravity="right"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btn_time_remind"
                    android:layout_width="54dp"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/holo_green_dark"
                    android:elevation="0dp"
                    android:text="aktif"
                    android:textColor="@android:color/white" />

                <Button
                    android:id="@+id/btn_time_remind2"
                    android:layout_width="wrap_content"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/darker_gray"
                    android:text="non aktif"
                    android:textColor="@android:color/white" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1">

            <TextView
                android:id="@+id/tv_set_name4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:text="Lanjutkan setelah panggilan"
                android:textColor="@android:color/white" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="52dp"
                android:gravity="right"
                android:orientation="horizontal">


                <Button
                    android:id="@+id/btn_un_dz"
                    android:layout_width="54dp"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/holo_green_dark"
                    android:text="aktif"
                    android:textColor="@android:color/white" />

                <Button
                    android:id="@+id/btn_un_dz2"
                    android:layout_width="wrap_content"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/darker_gray"
                    android:elevation="0dp"
                    android:text="non aktif"
                    android:textColor="@android:color/white" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1">

            <TextView
                android:id="@+id/tv_set_name5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:text="Otomatis memilih waktu dzikir"
                android:textColor="@android:color/white" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="52dp"
                android:gravity="right"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/btn_auto_dz"
                    android:layout_width="54dp"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/holo_green_dark"
                    android:text="aktif"
                    android:textColor="@android:color/white" />

                <Button
                    android:id="@+id/btn_auto_dz2"
                    android:layout_width="wrap_content"
                    android:layout_height="25dip"
                    android:layout_gravity="center_vertical"
                    android:background="@android:color/darker_gray"
                    android:elevation="0dp"
                    android:text="non aktif"
                    android:textColor="@android:color/white" />
            </LinearLayout>
        </LinearLayout>

        <!--<Switch-->
            <!--android:id="@+id/sw_un_dz"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_below="@+id/tvStateDefault"-->
            <!--android:layout_centerHorizontal="true"-->
