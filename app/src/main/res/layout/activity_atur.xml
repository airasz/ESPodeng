            <!--android:layout_height="20dip"-->
            <!--android:src=""/>-->
        <!--<chicken.head.custom.seekbar-->

            <!--android:layout_width="20dip"-->
            <!--android:layout_height="20dip"-->
            <!--/>-->
    </LinearLayout>


    <!--</android.support.constraint.ConstraintLayout>-->
</RelativeLayout>